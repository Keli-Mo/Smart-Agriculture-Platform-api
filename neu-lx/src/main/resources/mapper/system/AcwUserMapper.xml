<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neu.system.mapper.AcwUserMapper">

    <resultMap type="AcwUser" id="AcwUserResult">
        <result property="id"    column="id"    />
        <result property="sysUserId"    column="sys_user_id"    />
        <result property="realName"    column="real_name"    />
        <result property="sex"    column="sex"    />
        <result property="age"    column="age"    />
        <result property="cardNo"    column="card_no"    />
        <result property="phone"    column="phone"    />
        <result property="residentialAddress"    column="residential_address"    />
        <result property="permanentAddress"    column="permanent_address"    />
        <result property="birthday"    column="birthday"    />
        <result property="nativePlace"    column="native_place"    />
        <result property="nation"    column="nation"    />
        <result property="state"    column="state"    />
        <result property="photoUrl"    column="photo_url"    />
        <result property="createTime"    column="create_time"    />
        <result property="createBy"    column="create_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="extendCol1"    column="extend_col_1"    />
        <result property="extendCol2"    column="extend_col_2"    />
        <result property="extendCol3"    column="extend_col_3"    />
        <result property="extendCol4"    column="extend_col_4"    />
    </resultMap>

    <sql id="selectAcwUserVo">
        select id, sys_user_id, real_name, sex, age, card_no, phone, residential_address, permanent_address, birthday, native_place, nation, state, photo_url, create_time, create_by, update_time, update_by, extend_col_1, extend_col_2, extend_col_3, extend_col_4 from acw_user
    </sql>

    <select id="selectAcwUserList" parameterType="AcwUser" resultMap="AcwUserResult">
        <include refid="selectAcwUserVo"/>
        <where>
            <if test="sysUserId != null "> and sys_user_id = #{sysUserId}</if>
            <if test="realName != null  and realName != ''"> and real_name like concat('%', #{realName}, '%')</if>
            <if test="cardNo != null  and cardNo != ''"> and card_no = #{cardNo}</if>
        </where>
    </select>

    <select id="selectAcwUserById" parameterType="Long" resultMap="AcwUserResult">
        <include refid="selectAcwUserVo"/>
        where id = #{id}
    </select>

    <insert id="insertAcwUser" parameterType="AcwUser" useGeneratedKeys="true" keyProperty="id">
        insert into acw_user
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="sysUserId != null">sys_user_id,</if>
            <if test="realName != null">real_name,</if>
            <if test="sex != null">sex,</if>
            <if test="age != null">age,</if>
            <if test="cardNo != null">card_no,</if>
            <if test="phone != null">phone,</if>
            <if test="residentialAddress != null">residential_address,</if>
            <if test="permanentAddress != null">permanent_address,</if>
            <if test="birthday != null">birthday,</if>
            <if test="nativePlace != null">native_place,</if>
            <if test="nation != null">nation,</if>
            <if test="state != null">state,</if>
            <if test="photoUrl != null">photo_url,</if>
            <if test="createTime != null">create_time,</if>
            <if test="createBy != null">create_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="extendCol1 != null">extend_col_1,</if>
            <if test="extendCol2 != null">extend_col_2,</if>
            <if test="extendCol3 != null">extend_col_3,</if>
            <if test="extendCol4 != null">extend_col_4,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="sysUserId != null">#{sysUserId},</if>
            <if test="realName != null">#{realName},</if>
            <if test="sex != null">#{sex},</if>
            <if test="age != null">#{age},</if>
            <if test="cardNo != null">#{cardNo},</if>
            <if test="phone != null">#{phone},</if>
            <if test="residentialAddress != null">#{residentialAddress},</if>
            <if test="permanentAddress != null">#{permanentAddress},</if>
            <if test="birthday != null">#{birthday},</if>
            <if test="nativePlace != null">#{nativePlace},</if>
            <if test="nation != null">#{nation},</if>
            <if test="state != null">#{state},</if>
            <if test="photoUrl != null">#{photoUrl},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="extendCol1 != null">#{extendCol1},</if>
            <if test="extendCol2 != null">#{extendCol2},</if>
            <if test="extendCol3 != null">#{extendCol3},</if>
            <if test="extendCol4 != null">#{extendCol4},</if>
        </trim>
    </insert>

    <update id="updateAcwUser" parameterType="AcwUser">
        update acw_user
        <trim prefix="SET" suffixOverrides=",">
            <if test="sysUserId != null">sys_user_id = #{sysUserId},</if>
            <if test="realName != null">real_name = #{realName},</if>
            <if test="sex != null">sex = #{sex},</if>
            <if test="age != null">age = #{age},</if>
            <if test="cardNo != null">card_no = #{cardNo},</if>
            <if test="phone != null">phone = #{phone},</if>
            <if test="residentialAddress != null">residential_address = #{residentialAddress},</if>
            <if test="permanentAddress != null">permanent_address = #{permanentAddress},</if>
            <if test="birthday != null">birthday = #{birthday},</if>
            <if test="nativePlace != null">native_place = #{nativePlace},</if>
            <if test="nation != null">nation = #{nation},</if>
            <if test="state != null">state = #{state},</if>
            <if test="photoUrl != null">photo_url = #{photoUrl},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="extendCol1 != null">extend_col_1 = #{extendCol1},</if>
            <if test="extendCol2 != null">extend_col_2 = #{extendCol2},</if>
            <if test="extendCol3 != null">extend_col_3 = #{extendCol3},</if>
            <if test="extendCol4 != null">extend_col_4 = #{extendCol4},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteAcwUserById" parameterType="Long">
        delete from acw_user where id = #{id}
    </delete>

    <delete id="deleteAcwUserByIds" parameterType="String">
        delete from acw_user where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>